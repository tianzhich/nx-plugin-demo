{
  "npmScope": "org",
  "affected": {
    "defaultBase": "master"
  },
  "implicitDependencies": {
    "workspace.json": "*",
    "package.json": {
      "dependencies": "*",
      "devDependencies": "*"
    },
    "tsconfig.json": "*",
    "tslint.json": "*",
    "nx.json": "*"
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/workspace/tasks-runners/default",
      "options": {
        "cacheableOperations": ["build", "lint", "test", "e2e"]
      }
    }
  },
  "projects": {
    "plugin": {
      "tags": []
    },
    "plugin-e2e": {
      "tags": [],
      "implicitDependencies": ["plugin"]
    },
    "my-app": {
      "tags": []
    },
    "my-app-e2e": {
      "tags": [],
      "implicitDependencies": ["my-app"]
    }
  },
  "workspaceLayout": {
    "libsDir": "packages"
  }
}
